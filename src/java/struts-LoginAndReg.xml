<struts>
    <!-- Add your configuration elements here -->
     <package name="loginAndReg" extends="struts-default" namespace="">       
        
        <action name="Login" class="loginAction">  
                <result name="input">/Login.jsp</result>                 
                <result name="success">/infocenter.jsp</result>  
        </action> 
        <action name="Register" class="registerAction">             
                <result name="success">/Login.jsp</result>  
                <result name="input">/register.jsp</result>  
        </action>
        
        <action name="FirstAjaxAction" class="org.uu.controller.FirstAjaxAction">             
                <result name="success">/welcome.jsp</result>   
        </action>
        
       <action name="SearchUser" class="searchUser">             
              <result name="success">/searchResultList.jsp</result>   
        </action>
    </package>
    
    <package name="ajax" extends="json-default" namespace="">
        <action name="Searchp" class="searchPeople">             
            <result type="json"/>
            <result name="error">/welcome.jsp</result> 
        </action>
        
         
        
    </package>
</struts>
